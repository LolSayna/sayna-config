{
    "terminal.integrated.gpuAcceleration": "off",
    "window.zoomLevel": 1,
    "cpplint.cpplintPath": "/usr/bin/cpplint",
    "makefile.makefilePath": "test-stream",
    "terminal.integrated.fontFamily": "'JetBrainsMonoNL Nerd Font Mono'",
    "mesonbuild.downloadLanguageServer": true,
    "rust-analyzer.check.allTargets": false,
    "rust-analyzer.cargo.target": "build/i686-rstubs-kernel.json",
    "workbench.colorTheme": "Default Dark+",
    "git.confirmSync": false,
    "RainbowBrackets.depreciation-notice": false,
    "vscode-neovim.neovimExecutablePaths.linux": "/home/sayna/neovim/squashfs-root/usr/bin/nvim",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "chat.commandCenter.enabled": false,
    "[typst]": {
        "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "[typst-code]": {
        "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },
    "task.problemMatchers.neverPrompt": {
        "shell": true
    },
    "workbench.editor.wrapTabs": true,
    "debug.onTaskErrors": "abort",
    "cSpell.language": "en,de-de",
    "cSpell.userWords": [
        "Mareike"
    ],
    "editor.minimap.enabled": false,
    "[markdown]": {

        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false,
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        }
    },

}